{
  "name": "Omnia.Breaking.Web",
  "version": "0.1.0",
  "scripts": {
    "omnia-watch": "tsc -v && tsc -w",
    "test": "jest",
    "build": "tsc -v && tsc && omniafx build",
    "build-release": "npm run build -- --docs --minify"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@omnia/fx-models": "6.0.0",
    "@omnia/fx-sp": "6.0.0",
    "@omnia/fx-spfx": "6.0.0",
    "@omnia/tooling-vue": "6.0.0",
    "@omnia/wcm": "6.0.1",
    "@omnia/workplace": "6.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx|jsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "(@omnia/fx/)": "<rootDir>/__mocks__/index.ts"
    },
    "testRegex": "(/.*|\\.(test|spec))\\.(test.ts|test.tsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/wwwroot",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node"
  }
}
